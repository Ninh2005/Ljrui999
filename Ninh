#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
struct Date{
  int day,month,year;  
};
struct BuuKien{
  	int ma;
    char nguoigui[50];
    char nguoinhan[50];
    float trongluong;
    Date date;
    char noidung[100];
    int gia;
};
typedef BuuKien BK;

void NhapCT(BK arr[], int n)
{
	for(int i=0;i<n;i++)
	{
    scanf("%d",&arr[i].ma);
    getchar();
    fgets(arr[i].nguoigui,50,stdin);
    if(arr[i].nguoigui[strlen(arr[i].nguoigui)-1]=='\n')
        arr[i].nguoigui[strlen(arr[i].nguoigui)-1]='\0';
    fgets(arr[i].nguoinhan,50,stdin);
    if(arr[i].nguoinhan[strlen(arr[i].nguoinhan)-1]=='\n')
        arr[i].nguoinhan[strlen(arr[i].nguoinhan)-1]='\0';
	scanf("%f",&arr[i].trongluong);
    getchar();
   	scanf("%d %d %d", &arr[i].date.day, &arr[i].date.month, &arr[i].date.year);
	getchar();
    fgets(arr[i].noidung,100,stdin);
    if(arr[i].noidung[strlen(arr[i].noidung)-1]=='\n')
        arr[i].noidung[strlen(arr[i].noidung)-1]='\0';
    scanf("%d",&arr[i].gia);
    getchar(); }
}
bool TimKiem(BK arr[], int n)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i].date.month == 1 && arr[i].date.year == 2023)
        {
            return true;
        }
    }
    return false;
}
void XuatCT(BK arr[], int n)
{
    int dem = 0;
    for(int i = 0; i < n; i++)
    {
        if(arr[i].date.month == 1 && arr[i].date.year == 2023)
        {
            printf("%d - %s - %s - %.2f - %d/%d/%d - %s - %d\n", arr[i].ma, arr[i].nguoigui, arr[i].nguoinhan,
                   arr[i].trongluong, arr[i].date.day, arr[i].date.month, arr[i].date.year, arr[i].noidung,
                   arr[i].gia);
            dem++;
        }
    }
    if(dem == 0)
    {
        printf("Not Found");
    }
}

int main() {
	int n;
	do{
		scanf("%d",&n);
		getchar();
	}while(n < 1 || n > 100);
	BK arr[n];
    NhapCT(arr,n);
    XuatCT(arr,n);
    return 0;
}

