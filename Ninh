#include <stdio.h>
#include <math.h>
bool Ktra(int x)
{
    if(x<2) return false;
    for(int i=2;i<=sqrt(x);i++)
    {
        if(x%i==0) return false;
    }
    return true;
}
void NhapMatrix(int matrix[][100], int row,int column)
{
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<column;j++)
        {
            scanf("%d",&matrix[i][j]);
        }
    }
}
void dem(int matrix[][100], int row, int column )
{
    int dem=0;
    int bot = 0;
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<column;j++)
        {
            if(Ktra(matrix[i][j]))
            {
                dem++;
                bot = 1;
            }
        }
    }
    if(bot == 0)
        printf("None");
    else printf("%d", dem);
}

int main()
{
    int row,column;
    do{
        scanf("%d",&row);
    }while(row<=0);
    do{
        scanf("%d",&column);
    }while(column>100);
    int matrix[100][100];
    NhapMatrix(matrix,row,column);
    dem(matrix,row,column);
	return 0;
}
